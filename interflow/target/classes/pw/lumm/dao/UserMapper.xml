<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pw.lumm.dao.UserMapper" >
    <insert id="insertUser" parameterType="pw.lumm.model.User" >
        insert into tb_user (id, username, sex) values (#{id}, #{username}, #{sex})
    </insert>

</mapper>